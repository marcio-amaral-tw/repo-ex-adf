{
	"name": "ConfFilePipeline",
	"properties": {
		"activities": [
			{
				"name": "Read conf",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "conf_json",
						"type": "DatasetReference",
						"parameters": {
							"p_container_name": {
								"value": "@pipeline().globalParameters.container_path",
								"type": "Expression"
							},
							"p_folder_name": {
								"value": "@pipeline().globalParameters.config_folder_path",
								"type": "Expression"
							},
							"p_file_name": {
								"value": "@pipeline().globalParameters.config_file_path",
								"type": "Expression"
							}
						}
					}
				}
			}
		],
		"folder": {
			"name": "examples-pipelines"
		},
		"annotations": []
	}
}