{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['Nome']"
								},
								"sink": {
									"name": "Nome"
								}
							},
							{
								"source": {
									"path": "$['Sobrenome']"
								},
								"sink": {
									"name": "Sobrenome"
								}
							},
							{
								"source": {
									"path": "$['Cidade']"
								},
								"sink": {
									"name": "Cidade"
								}
							},
							{
								"source": {
									"path": "['tipo']"
								},
								"sink": {
									"name": "tipo"
								}
							},
							{
								"source": {
									"path": "['Numero']"
								},
								"sink": {
									"name": "Numero"
								}
							}
						],
						"collectionReference": "$['Telefones']",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "ds_json",
						"type": "DatasetReference",
						"parameters": {
							"p_container_name": "datahub",
							"p_folder_name": "teste_data/input",
							"p_file_name": "cliente.json"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_csv_filename",
						"type": "DatasetReference",
						"parameters": {
							"container_name": "datahub",
							"folder_name": "teste_data/output",
							"file_name": "cliente.csv"
						}
					}
				]
			}
		],
		"folder": {
			"name": "examples-pipelines"
		},
		"annotations": []
	}
}